<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.archana.weathertest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.archana.weathertest.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.archana.weathertest.Theme" />
        <activity
            android:name="com.archana.weathertest.CityActivity"
            android:label="@string/title_activity_city"
            android:theme="@style/AppTheme" />
        <activity android:name="com.archana.weathertest.SearchActivity" />
        <activity android:name="com.archana.weathertest.MainActivity"></activity>
    </application>

</manifest>